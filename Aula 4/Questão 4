Questão 4 
#include <stdio.h>
#include<locale.h>

int main() {
    setlocale(LC_ALL,"portuguese");
     int hora , min ;
    printf("\nDigite as horas:");
    scanf("%d" , &hora);
    
    printf("\nDigite os minutos:");
    scanf("%d" ,&min);
    
    printf("\nTotal de minutos %c %d\n",130, (hora*60)+min);
    
    return 0;
}